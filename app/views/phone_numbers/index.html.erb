<h1>Listing Phone Numbers</h1>

<table>
  <tr>
    <th>Phone Number</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @phone_numbers.each do |phone_number| %>
  <tr>
    <td><%= phone_number.pnum %></td>
    <td><%= phone_number.location %></td>
    <td><%= link_to 'Show', phone_number %></td>
    <td><%= link_to 'Edit', edit_phone_number_path(phone_number) %></td>
    <td><%= link_to 'Delete', phone_number, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Phone Number', new_phone_number_path %>
